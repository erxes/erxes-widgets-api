{
  "name": "GraphQL API for erxes widgets on Heroku",
  "description": "GraphQL API for erxes widgets",
  "keywords": [
    "Marketing",
    "sales",
    "customer engagement",
    "customer support",
    "CRM",
    "node",
    "express",
    "graphql",
    "apollo"
  ],
  "website": "https://erxes.io",
  "repository": "https://github.com/erxes/erxes-widgets-api",
  "logo": "https://erxes.io/static/images/logo/logo-dark.png",
  "success_url": "/",
  "env": {
    "PORT": {
      "description": "A port number that erxes api will be running on",
      "value": "3100"
    },
    "WIDGET_URL": {
      "description": "Erxes Widget APP URL",
      "value": "https://ERXESWIDGETSAPPNAME.herokuapp.com"
    },
    "MONGO_URL": {
      "description": "MONGO URL",
      "value": "mongodb://localhost/erxes"
    },
    "RABBITMQ_HOST": {
      "description": "RABBITMQ_HOST URL",
      "value": "amqp://localhost"
    }
  },
